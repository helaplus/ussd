{
    "name": "helaplus/ussd",
    "description": "A laravel package to make it simpler to create USSD applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Le-Yo",
            "email": "leo@helaplus.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Helaplus\\Ussd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Helaplus\\Ussd\\Tests\\": "tests/"
        }
